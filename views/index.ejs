<!DOCTYPE html>
<html ng-app="Mastering-Data-Mining-and-Analytics">
    <head>
        <title><%= title %></title>
        <meta charset="UTF-8">
        <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
        <link rel="icon" href="images/favicon.ico" type="image/x-icon">
        <!--Script Module-->
        <% dependances.scripts.forEach(function(script, index) { %>
        <script src="<%= script %>.js"></script>
        <% }); %>

        <!--CSS Module-->
        <% dependances.css.forEach(function(css, index) { %>
        <link rel="stylesheet" type="text/css" href="<%= css %>.css">
        <% }); %>

    </head>
<body ng-controller="AppCtrl" layout="row">
    <div flex="100">
        <div layout="row">
            <div layout="column" layout-fill>
                <top-bar flex="100" ></top-bar>
                <right-bar></right-bar>
            </div>
        </div>
        <div flex="100" >
            <representation ></representation>
        </div>
    </div>
</body>
</html>
